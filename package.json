{
  "name": "vue-json-form",
  "version": "0.0.1",
  "homepage": "https://github.com/springalskey/vue-json-form",
  "main": "src/components/index.js",
  "scripts": {
    "start": "npm run serve",
    "serve": "cross-env ZX_ENV=dev vue-cli-service serve",
    "lint": "vue-cli-service lint",
    "lib": "cross-env ZX_ENV=prod vue-cli-service build --target lib --name vue-json-form/index --dest lib src/components/index.js"
  },
  "dependencies": {
    "@vue-json/vue-json-form": "0.0.1",
    "axios": "0.21.1",
    "core-js": "3.6.5",
    "crypto-js": "3.1.2",
    "element-ui": "2.15.1",
    "user": "0.0.0",
    "vue": "2.6.11",
    "vue-router": "3.5.1"
  },
  "devDependencies": {
    "@vue/cli": "4.5.11",
    "@vue/cli-plugin-babel": "4.5.0",
    "@vue/cli-plugin-eslint": "4.5.0",
    "@vue/cli-service": "4.5.0",
    "babel-eslint": "10.1.0",
    "cross-env": "^7.0.3",
    "eslint": "5.16.0",
    "eslint-config-standard": "12.0.0",
    "eslint-import-resolver-webpack": "0.8.4",
    "eslint-plugin-html": "5.0.5",
    "eslint-plugin-import": "2.17.3",
    "eslint-plugin-node": "8.0.1",
    "eslint-plugin-promise": "4.1.1",
    "eslint-plugin-standard": "4.0.0",
    "eslint-plugin-vue": "5.2.2",
    "husky": "1.3.1",
    "lint-staged": "8.1.6",
    "node-sass": "4.12.0",
    "sass-loader": "8.0.0",
    "vue-template-compiler": "2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "./src/**/*.{js,vue}": [
      "eslint --fix",
      "git add"
    ]
  }
}
